<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Lemonade" type="Lemonade.LemonadeConfigurationSection, Lemonade"/>
  </configSections>

  <Lemonade FeatureResolver="Lemonade.AppConfigFeatureResolver" 
            ConfigurationResolver="Lemonade.AppConfigConfigurationResolver" 
            ApplicationName="Hello World" 
            CacheExpiration="1" />

  <appSettings>
    <add key="EnabledProperty" value="true"/>
    <add key="DisabledProperty" value="false"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
